<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
	   xmlns:xsi ="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation = "http://www.springframework.org/schema/beans 
	    http://www.springframework.org/schema/beans/spring-beans.xsd"
>
	<!-- Service  -->
	<bean id = "youngupLogService" class ="Service.Youngup.YoungupLogService"/><!-- 영업 사원 로그인 실행 서비스-->
	<bean id = "exListSer" class ="Service.Youngup.ExplorListService"/><!-- 사전답사 보고서 리스트 서비스-->
	<bean id = "newComSer" class ="Service.Youngup.NewCompanyService"/><!-- 영업부 신규업체 리스트 보기-->
	<bean id = "expWriteSer" class ="Service.Youngup.ExplorWriteService"/><!-- 신규 사전답사 보고서 작성 서비스--> 
	<bean id = "expInSer" class ="Service.Youngup.ExplorIncomService"/><!-- 미완료 사전답사 보고서 서비스-->
	<bean id = "expComSer" class ="Service.Youngup.ExplorComService"/><!-- 완료 사전답사 보고서 서비스-->
	<bean id = "youngMainSer" class ="Service.Youngup.YoungupMainService"/><!-- 영업 메인 업무표 서비스-->
	<bean id = "youngScheSer" class ="Service.Youngup.YoungupScheduleService"/><!-- 영업 사전답사 일정--> 
	
	 

   	<!-- Repositopry -->
	<bean id = "youngSelRepo" class ="Repository.Youngup.YoungupSelectRepository"/><!-- 영업부 조회 repository--> 
	<bean id = "youngInRepo" class ="Repository.Youngup.YoungupInsertRepository"/><!-- 영업부 insert repository-->
	<bean id = "youngUpRepo" class ="Repository.Youngup.YoungupUpdateRepository"/><!-- 영업부 update repository--> 
</beans>


