<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
	   xmlns:xsi ="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation = "http://www.springframework.org/schema/beans 
	    http://www.springframework.org/schema/beans/spring-beans.xsd"
>
	<bean class="Controller.Main.MainController"/> <!-- 초기 화면 가는 컨트롤러 -->
	<bean class="Controller.StaffLog.StaffLogController"/> <!-- 직원 로그인 창 가는 컨트롤러-->
	<bean class="Controller.User.UserLoginController"/> <!-- 일반 회원이 로그인 하는 컨트롤러-->
	<bean class="Controller.User.UserFormController"/> <!-- 회원가입 컨트롤러-->
	<bean class="Controller.User.LogController"/> <!-- 회원,가이드 ,업체 로그인 창 가는 컨트롤러-->
	<bean class="Controller.User.CompanyFormController"/> <!-- 업체 등록 신청서 작성 컨트롤러 -->
	<bean class="Controller.User.CompanyLoginController"/> <!-- 업체가 로그인 하는 컨트롤러 -->
	<bean class="Controller.User.CompanyLogoutController"/> <!-- 업체가 로그아웃 하는 컨트롤러 -->
	<bean class="Controller.Company.CompanyMainController"/> <!-- 업체 메인으로 가는 컨트롤러-->
	<bean class="Controller.User.UserMainController" /> <!-- 회원 메인으로 가는 컨트롤러 -->
</beans>