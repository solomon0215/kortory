<mapper namespace="EmployeeMapper">

<cache />
<sql id="baseEmployee">
	EMPLOYEE_NUM, EMPLOYEE_ID, EMPLOYEE_PW, EMPLOYEE_BIRTH, EMPLOYEE_NAME,
	EMPLOYEE_GENDER, EMPLOYEE_PH, EMPLOYEE_REGIST, EMPLOYEE_EMAIL
