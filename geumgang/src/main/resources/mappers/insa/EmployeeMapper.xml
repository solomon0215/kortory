		<result column="" jdbcType = "DATE" property="Birth"/>
		<result column="" jdbcType="VARCHAR" property="Name"/>		
		<result column="" jdbcType="CHAR" property="Gender"/>
		<result column="" jdbcType="VARCHAR" property="Ph"/>
		<result column="" jdbcType="VARCHAR" property="Email"/>
		<result column="" jdbcType="DATE" property="Regist"/>		
	</resultMap>	
